{
  "name": "express_api",
  "version": "1.0.0",
  "description": "Teacher Hub API",
  "main": "index.js",
  "scripts": {
    "start-dev": "nodemon --exec ./node_modules/.bin/ts-node -- ./server.ts",
    "start": "node dist/server.js",
    "postinstall": "tsc",
    "test": "jest"
  },
  "author": "Fritz Jay",
  "license": "ISC",
  "engines": {
    "node": "8.11.3",
    "npm": "6.2.0"
  },
  "dependencies": {
    "@types/bcrypt": "^2.0.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.9",
    "@types/jsonwebtoken": "^7.2.8",
    "@types/mathjs": "^4.4.2",
    "@types/mongodb": "^3.1.14",
    "@types/mongoose": "^5.3.0",
    "@types/node": "^10.12.2",
    "bcrypt": "^3.0.2",
    "body-parser": "^1.18.3",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "express-graphql": "^0.7.1",
    "graphql": "^14.0.2",
    "jsonwebtoken": "^8.3.0",
    "mathjs": "^5.2.3",
    "mongoose": "^5.3.8",
    "shortid": "^2.2.13",
    "ts-node": "^7.0.1",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "jest": "^23.6.0",
    "nodemon": "^1.18.5",
    "ts-jest": "^22.4.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testPathIgnorePatterns": [
      "/node modules/",
      "dist"
    ],
    "testRegex": ".*spec.*.(js|ts|tsx)$$",
    "testURL": "https://localhost/"
  }
}
